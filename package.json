{
  "name": "pb-directory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint && vue-cli-service lint --ext e2e_tests/tests",
    "lint-fix": "vue-cli-service lint --fix",
    "e2e": "./node_modules/.bin/nightwatch e2e_tests/tests/ -c ./e2e_tests/conf/nightwatch.conf.js",
    "e2e:browserstack": "node ./e2e_tests/conf/local.runner.js -c ./e2e_tests/conf/browserstack.nightwatch.conf.js",
    "nightwatch:output": "xunit-viewer -r e2e_tests/output/ -o 'e2e_tests/output/output.html'"
  },
  "dependencies": {
    "algoliasearch": "4.3.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "browserstack-local": "^1.4.5",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "instantsearch.css": "7.1.0",
    "jsonwebtoken": "^8.5.1",
    "vue": "^2.6.11",
    "vue-instantsearch": "^3.0.3",
    "vue-router": "^3.3.4",
    "vue-server-renderer": "^2.6.11",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.4.5",
    "@vue/cli-service": "^4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^83.0.1",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-beta.2",
    "fibers": "^5.0.0",
    "geckodriver": "^1.20.0",
    "nightwatch": "^1.3.7",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "selenium-server": "^3.141.59",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
