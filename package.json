{
  "name": "pressbooks-directory",
  "version": "2.9.1",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:staging": "TAILWIND_MODE=build vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .js --ext .vue",
    "lint:fix": "eslint ./src --ext .js --ext .vue --fix",
    "start": "npm run dev & wait-on tcp:3001 -v",
    "test": "cypress open",
    "test:ci": "cypress run --headless"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vuepic/vue-datepicker": "^5.1.2",
    "algoliasearch": "^4.17.1",
    "dayjs": "^1.11.7",
    "flowbite": "^1.6.5",
    "instantsearch.js": "^4.56.0",
    "intro.js": "6.0.0",
    "nprogress": "^0.2.0",
    "resize-detector": "^0.3.0",
    "search-insights": "^2.6.0",
    "vue": "^3.3.4",
    "vue-instantsearch": "^4.10.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.1",
    "vue-select": "^4.0.0-beta.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@sentry/vue": "^7.54.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.3.4",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.12.0",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.1",
    "postcss": "^8.4.24",
    "postcss-nested": "^6.0.1",
    "tailwindcss": "^3.3.2",
    "vite": "^3.2.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-html-env": "^1.2.8"
  }
}