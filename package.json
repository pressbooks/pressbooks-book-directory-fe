{
  "name": "pressbooks-directory",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:staging": "TAILWIND_MODE=build vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .js --ext .vue",
    "lint:fix": "eslint ./src --ext .js --ext .vue --fix",
    "start": "npm run dev & wait-on tcp:3001 -v",
    "test": "cypress open",
    "test:ci": "cypress run --headless"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vuepic/vue-datepicker": "^3.4.0",
    "algoliasearch": "^4.14.2",
    "dayjs": "^1.11.3",
    "instantsearch.js": "^4.44.0",
    "intro.js": "^3.4.0",
    "nprogress": "^0.2.0",
    "resize-detector": "^0.3.0",
    "search-insights": "^2.2.1",
    "vue": "^3.2.37",
    "vue-instantsearch": "^4.4.2",
    "vue-lazyload": "^3.0.0-rc.2",
    "vue-router": "^4.1.3",
    "vue-select": "^4.0.0-beta.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@sentry/vue": "^7.10.0",
    "@vitejs/plugin-vue": "^3.0.2",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.4.0",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^3.1.7",
    "vite": "^3.0.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-html-env": "^1.2.4"
  }
}
